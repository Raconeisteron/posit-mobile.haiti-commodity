<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <!-- <RelativeLayout android:layout_width="fill_parent" android:id="@+id/rellayout" -->
        <!-- android:layout_height="wrap_content" android:orientation="horizontal"> -->

        <TextView
            android:id="@+id/guid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="guid"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@+id/guid"
            android:text="id#"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/synced"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@+id/id"
            android:text="synced"
            android:visibility="gone"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/in_prompt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/guid"
            android:text="@string/cItem"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/commodity_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/guid"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/in_prompt"
            android:singleLine="true"
            android:textSize="13dp" />



        <!-- <TextView android:id="@+id/idNumberText" -->
        <!-- android:layout_width="wrap_content" android:layout_height="wrap_content" -->
        <!-- android:layout_marginLeft="2dp" android:layout_marginRight="2dp" -->
        <!-- android:layout_below="@id/status" android:textSize="13dp" -->
        <!-- android:layout_alignParentRight="true" android:layout_alignTop="@id/longitude" /> -->
        <!-- </RelativeLayout> -->


        <!-- <CheckBox -->
        <!-- android:id="@+id/Sent" -->
        <!-- android:layout_width="wrap_content" -->
        <!-- android:layout_height="wrap_content" -->
        <!-- android:layout_alignParentBottom="true" -->
        <!-- android:layout_alignParentRight="true" -->
        <!-- android:text="@string/csenddata" /> -->

        <TextView
            android:id="@+id/out_prompt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/in_prompt"
            android:text="@string/cLocation2"
            android:textSize="13dp" />

                <TextView
                    android:id="@+id/market_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
            		android:layout_below="@+id/in_prompt"
            		android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/out_prompt"
                    android:singleLine="true"
                    android:textSize="13dp" />



    <requestFocus>
    </requestFocus>

    <TextView
        android:id="@+id/timestamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
        android:layout_below="@+id/out_prompt"
        android:text="time/date"
        android:visibility="gone"
        android:textSize="13dp" />

    <TextView
        android:id="@+id/latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
		android:layout_below="@+id/timestamp"
        android:text="lat"
        android:visibility="gone"
        android:textSize="13dp" />

    <TextView
        android:id="@+id/longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
		android:layout_below="@+id/latitude"
        android:text="long"
        android:visibility="gone"
        android:textSize="13dp" />

<!--     <CheckBox -->
<!--         android:id="@+id/checkBox1" -->
<!--         android:layout_width="wrap_content" -->
<!--         android:layout_height="wrap_content" -->
<!--         android:layout_alignParentRight="true" -->
<!--         android:layout_below="@+id/longitude" -->
<!--         android:text="@string/csenddata" /> -->

    </RelativeLayout>
</LinearLayout>